#: kalarmdir/settingsdialog.ui:128
#, kde-format
msgid "Read only"
msgstr "唯讀"

#. i18n: ectx: property (text), widget (QLabel, label_2)
#: kalarmdir/settingsdialog.ui:135
#, kde-format
