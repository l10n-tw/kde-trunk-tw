msgid_plural "%1 devices found"
msgstr[0] "找到 %1 個裝置"

#: kdeconnect-cli.cpp:137
#, kde-format
msgid "No devices found"
msgstr "沒有找到任何裝置"

#: kdeconnect-cli.cpp:151
#, kde-format
msgid "(unpaired)"
msgstr "（未配對）"

#: kdeconnect-cli.cpp:185
#, kde-format
msgid "No device specified"
msgstr "未指定裝置"

#: kdeconnect-cli.cpp:214
#, kde-format
msgid "Shared %1"
msgstr "已分享 %1"

#: kdeconnect-cli.cpp:220
#, kde-format
msgid "Shared text: %1"
msgstr "已分享文字：%1"

#: kdeconnect-cli.cpp:226
#, kde-format
msgid "waiting for device..."
msgstr "等待裝置中..."

#: kdeconnect-cli.cpp:240
#, kde-format
msgid "Device not found"
msgstr "沒有找到裝置"

#: kdeconnect-cli.cpp:242
#, kde-format
msgid "Already paired"
msgstr "已經配對"

#: kdeconnect-cli.cpp:244
#, kde-format
msgid "Pair requested"
msgstr "已請求配對"

#: kdeconnect-cli.cpp:251
#, kde-format
msgid "Already not paired"
msgstr "已經沒有取消配對"

#: kdeconnect-cli.cpp:253
#, kde-format
msgid "Unpaired"
msgstr "未配對"

#: kdeconnect-cli.cpp:269
#, kde-format
msgid ""
"error: should specify the SMS's recipient by passing --destination <phone "
"number>"
msgstr "錯誤：需要透過指定 --destination <電話號碼> 來指定簡訊的接收者"

